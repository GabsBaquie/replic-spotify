{
  "expo": {
    "name": "replic-spotify",
    "slug": "replic-spotify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "replicspotify",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.replicspotify.replicspotify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "LSApplicationQueriesSchemes": ["spotify", "spotify-action"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.replicspotify.replicspotify",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "replicspotify",
              "host": "callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-audio",
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f5afc72f-92d1-47ac-86cd-25b02da81211"
      }
    },
    "owner": "replic-spotify",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/f5afc72f-92d1-47ac-86cd-25b02da81211"
    }
  }
}
